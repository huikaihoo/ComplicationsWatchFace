<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/watch_face_background"
        android:layout_width="@dimen/settings_preview_size"
        android:layout_height="@dimen/settings_preview_size"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/preview_config_background" />

    <ImageView
        android:id="@+id/complication_top_bg"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/watch_face_background"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true"
        android:background="@android:color/transparent"
        android:src="@drawable/added_complication" />

    <ImageButton
        android:id="@+id/complication_top"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/watch_face_background"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true"
        android:background="@android:color/transparent" />

    <ImageView
        android:id="@+id/complication_top_left_bg"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/watch_face_background"
        android:layout_marginTop="60dp"
        android:background="@android:color/transparent"
        android:src="@drawable/added_complication" />

    <ImageButton
        android:id="@+id/complication_top_left"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/watch_face_background"
        android:layout_marginTop="60dp"
        android:layout_below="@+id/complication_top_bg"
        android:layout_marginBottom="10dp"
        android:background="@android:color/transparent" />

    <ImageView
        android:id="@+id/complication_top_right_bg"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/complication_top_right"
        android:layout_alignTop="@+id/complication_top_left_bg"
        android:background="@android:color/transparent"
        android:src="@drawable/added_complication" />

    <ImageButton
        android:id="@+id/complication_top_right"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/watch_face_background"
        android:layout_alignTop="@+id/complication_top_right_bg"
        android:background="@android:color/transparent" />

    <ImageView
        android:id="@+id/complication_bottom_left_bg"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/watch_face_background"
        android:layout_below="@+id/complication_top_left_bg"
        android:background="@android:color/transparent"
        android:src="@drawable/added_complication" />

    <ImageButton
        android:id="@+id/complication_bottom_left"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/watch_face_background"
        android:layout_below="@+id/complication_top_left_bg"
        android:background="@android:color/transparent" />

    <ImageView
        android:id="@+id/complication_bottom_right_bg"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/watch_face_background"
        android:layout_below="@+id/complication_top_right_bg"
        android:background="@android:color/transparent"
        android:src="@drawable/added_complication" />

    <ImageButton
        android:id="@+id/complication_bottom_right"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/watch_face_background"
        android:layout_below="@+id/complication_top_right_bg"
        android:background="@android:color/transparent" />

    <ImageView
        android:id="@+id/complication_bottom_bg"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/watch_face_background"
        android:layout_marginBottom="10dp"
        android:layout_centerHorizontal="true"
        android:background="@android:color/transparent"
        android:src="@drawable/added_complication" />

    <ImageButton
        android:id="@+id/complication_bottom"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/watch_face_background"
        android:layout_marginBottom="10dp"
        android:layout_centerHorizontal="true"
        android:background="@android:color/transparent" />

    <ImageView
        android:id="@+id/complication_count_bg"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@android:color/transparent"
        android:src="@drawable/added_complication" />

    <ImageButton
        android:id="@+id/complication_count"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@android:color/transparent" />

</RelativeLayout>